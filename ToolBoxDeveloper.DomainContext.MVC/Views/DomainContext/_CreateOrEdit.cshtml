@model ToolBoxDeveloper.DomainContext.Domain.Dto.DomainContextDto
<div class="row">
    <div class="col-md-4">
        <label asp-for="Organization" class="control-label"></label>
        <input asp-for="Organization" placeholder="ToolBoxDeveloper" class="form-control" />
        <span asp-validation-for="Organization" class="text-danger"></span>
    </div>
    <div class="col-md-4">
        <label asp-for="Domain" class="control-label"></label>
        <input asp-for="Domain" placeholder="DomainContext" class="form-control" />
        <span asp-validation-for="Domain" class="text-danger"></span>
    </div>
    <div class="col-md-4">
        <label asp-for="Context" class="control-label"></label>
        <input asp-for="Context" placeholder="Domain" class="form-control" />
        <span asp-validation-for="Context" class="text-danger"></span>
    </div>
    <div class="col-md-4">
        <label asp-for="Key" class="control-label"></label>
        <input asp-for="Key" placeholder="Context" class="form-control" />
        <span asp-validation-for="Key" class="text-danger"></span>
    </div>
    <div class="col-md-4">
        <label asp-for="Description" class="control-label"></label>
        <input asp-for="Description" placeholder="Contexto" class="form-control" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
</div>
<div class="row row-site">
    <div class="col-md-4">
        <a asp-action="Index" class="btn btn-info btn-site">
            <i class="fa fa-backward" aria-hidden="true"></i>
            Voltar
        </a>
    </div>
    <div class="col-md-4">
        <button type="submit" class="btn btn-success btn-site">
            <i class="fa fa-floppy-o" aria-hidden="true"></i>
            Salvar
        </button>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }
